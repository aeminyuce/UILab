{
  "name": "ui",
  "scripts": {
    "css-build": "npm run css & npm run css-helpers & npm run css-custom",
    "css": "lessc less/ui.less dist/css/ui.css",
    "css-helpers": "npm run css-iconlist",
    "css-iconlist": "lessc less/helpers/iconlist.less dist/css/iconlist.css",
    "css-custom": "npm run css-dashboard & npm run css-error-pages & npm run css-landing-page & npm run css-login-page & npm run css-movie-app & npm run css-pricing-table & npm run css-product-listing & npm run css-sign-up-page",
    "css-dashboard": "lessc less/custom/dashboard.less dist/css/dashboard.css",
    "css-error-pages": "lessc less/custom/error-pages.less dist/css/error-pages.css",
    "css-landing-page": "lessc less/custom/landing-page.less dist/css/landing-page.css",
    "css-login-page": "lessc less/custom/login-page.less dist/css/login-page.css",
    "css-movie-app": "lessc less/custom/movie-app.less dist/css/movie-app.css",
    "css-pricing-table": "lessc less/custom/pricing-table.less dist/css/pricing-table.css",
    "css-product-listing": "lessc less/custom/product-listing.less dist/css/product-listing.css",
    "css-sign-up-page": "lessc less/custom/sign-up-page.less dist/css/sign-up-page.css",
    "js-build": "npm run js & npm run js-helpers & npm run js-custom",
    "js": "rollup js/ui.js --file dist/js/ui.js & babel dist/js/ui.js --out-file dist/js/ui.js --no-comments",
    "js-helpers": "npm run js-classnames & npm run js-iconlist",
    "js-classnames": "babel js/helpers/classnames.js --out-file dist/js/classnames.js --no-comments",
    "js-iconlist": "babel js/helpers/iconlist.js --out-file dist/js/iconlist.js --no-comments",
    "js-custom": "npm run js-dashboard & npm run js-landing-page & npm run js-product-listing & npm run js-real-estate-details & npm run js-useragents",
    "js-dashboard": "babel js/custom/dashboard.js --out-file dist/js/dashboard.js --no-comments",
    "js-landing-page": "babel js/custom/landing-page.js --out-file dist/js/landing-page.js --no-comments",
    "js-product-listing": "babel js/custom/product-listing.js --out-file dist/js/product-listing.js --no-comments",
    "js-real-estate-details": "babel js/custom/real-estate-details.js --out-file dist/js/real-estate-details.js --no-comments",
    "js-useragents": "babel js/custom/useragents.js --out-file dist/js/useragents.js --no-comments"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "eslint": "^8.4.1",
    "less": "^4.1.2",
    "rollup": "^2.61.1"
  }
}
