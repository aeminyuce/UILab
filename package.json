{
  "name": "uilab",
  "title": "UI lab",
  "description": "UI lab is a modular design system for developing web interfaces fastly!",
  "version": "1.0.0",
  "license": "MIT",
  "style": "css/ui.css",
  "less": "css/ui.less",
  "main": "js/ui.js",
  "scripts": {
    "css-build": "npm run css:min & npm run css-helpers & npm run css-custom",
    "css": "lessc css/ui.less css/ui.css",
    "css:min": "lessc css/ui.less css/ui.css --compress",
    "css-helpers": "npm run css-iconlist",
    "css-iconlist": "lessc css/helpers/iconlist.less css/helpers/iconlist.css",
    "css-custom": "npm run css-dashboard & npm run css-error-pages & npm run css-landing-page & npm run css-login-page & npm run css-movie-app & npm run css-pricing-table & npm run css-product-listing & npm run css-sign-up-page",
    "css-dashboard": "lessc css/custom/dashboard.less css/custom/dashboard.css",
    "css-error-pages": "lessc css/custom/error-pages.less css/custom/error-pages.css",
    "css-landing-page": "lessc css/custom/landing-page.less css/custom/landing-page.css",
    "css-login-page": "lessc css/custom/login-page.less css/custom/login-page.css",
    "css-movie-app": "lessc css/custom/movie-app.less css/custom/movie-app.css",
    "css-pricing-table": "lessc css/custom/pricing-table.less css/custom/pricing-table.css",
    "css-product-listing": "lessc css/custom/product-listing.less css/custom/product-listing.css",
    "css-sign-up-page": "lessc css/custom/sign-up-page.less css/custom/sign-up-page.css",
    "js-build": "npm run js:min & npm run js-helpers & npm run js-custom",
    "js": "babel js/core.js js/modules/*.js --out-file js/ui.js",
    "js:es6": "babel --no-babelrc js/core.js js/modules/*.js --out-file js/ui.js",
    "js:min": "babel js/core.js js/modules/*.js --out-file js/ui.js --minified --no-comments",
    "js-helpers": "npm run js-classnames & npm run js-iconlist",
    "js-classnames": "babel js/helpers/classnames.js --out-file js/helpers/classnames-bundle.js --no-comments",
    "js-iconlist": "babel js/helpers/iconlist.js --out-file js/helpers/iconlist-bundle.js --no-comments",
    "js-custom": "npm run js-dashboard & npm run js-landing-page & npm run js-product-listing & npm run js-real-estate-details & npm run js-useragents",
    "js-dashboard": "babel js/custom/dashboard.js --out-file js/custom/dashboard-bundle.js --no-comments",
    "js-landing-page": "babel js/custom/landing-page.js --out-file js/custom/landing-page-bundle.js --no-comments",
    "js-product-listing": "babel js/custom/product-listing.js --out-file js/custom/product-listing-bundle.js --no-comments",
    "js-real-estate-details": "babel js/custom/real-estate-details.js --out-file js/custom/real-estate-details-bundle.js --no-comments",
    "js-useragents": "babel js/custom/useragents.js --out-file js/custom/useragents-bundle.js --no-comments"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeminyuce/UILab.git"
  },
  "bugs": {
    "url": "https://github.com/aeminyuce/UILab/issues"
  },
  "homepage": "https://github.com/aeminyuce/UILab#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "eslint": "^8.4.1",
    "less": "^4.1.2"
  }
}
