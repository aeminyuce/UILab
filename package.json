{
  "name": "uilab-react",
  "description": "UI Lab is a modular design system for developing web interfaces fastly with React.",
  "homepage": "https://github.com/aeminyuce/uilab",
  "version": "1.0.0",
  "main": "js/ui.js",
  "style": "less/ui.less",
  "repository": {
    "type": "git",
    "url": "https://github.com/aeminyuce/uilab.git"
  },
  "keywords": [
    "react",
    "ui",
    "lab",
    "responsive",
    "design system",
    "ui examples",
    "web",
    "css",
    "less",
    "javascript"
  ],
  "files": [
    "components",
    "icon",
    "js/core",
    "js/modules",
    "less/core",
    "less/modules"
  ],
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --mode development",
    "test": "mocha tests/unit/*.test.js --require @babel/register",

    "icon:build": "npm run icon:svgo && npm run icon:sprite && npm run icon:react",

    "icon:svgo": "npm run icon:svgo:brands && npm run icon:svgo:commerce && npm run icon:svgo:files && npm run icon:svgo:general && npm run icon:svgo:kitchen && npm run icon:svgo:media && npm run icon:svgo:real-estate && npm run icon:svgo:social && npm run icon:svgo:touch && npm run icon:svgo:weather",
    "icon:svgo:brands": "svgo -f icon/svg/brands",
    "icon:svgo:commerce": "svgo -f icon/svg/commerce",
    "icon:svgo:files": "svgo -f icon/svg/files",
    "icon:svgo:general": "svgo -f icon/svg/general",
    "icon:svgo:kitchen": "svgo -f icon/svg/kitchen",
    "icon:svgo:media": "svgo -f icon/svg/media",
    "icon:svgo:real-estate": "svgo -f icon/svg/real-estate",
    "icon:svgo:social": "svgo -f icon/svg/social",
    "icon:svgo:touch": "svgo -f icon/svg/touch",
    "icon:svgo:weather": "svgo -f icon/svg/weather",

    "icon:sprite": "npm run icon:sprite:brands && npm run icon:sprite:commerce && npm run icon:sprite:files && npm run icon:sprite:general && npm run icon:sprite:kitchen && npm run icon:sprite:media && npm run icon:sprite:real-estate && npm run icon:sprite:social && npm run icon:sprite:touch && npm run icon:sprite:weather",
    "icon:sprite:brands": "svgstore -o icon/sprite/brands.svg icon/svg/brands/*.svg",
    "icon:sprite:commerce": "svgstore -o icon/sprite/commerce.svg icon/svg/commerce/*.svg",
    "icon:sprite:files": "svgstore -o icon/sprite/files.svg icon/svg/files/*.svg",
    "icon:sprite:general": "svgstore -o icon/sprite/general.svg icon/svg/general/*.svg",
    "icon:sprite:kitchen": "svgstore -o icon/sprite/kitchen.svg icon/svg/kitchen/*.svg",
    "icon:sprite:media": "svgstore -o icon/sprite/media.svg icon/svg/media/*.svg",
    "icon:sprite:real-estate": "svgstore -o icon/sprite/real-estate.svg icon/svg/real-estate/*.svg",
    "icon:sprite:social": "svgstore -o icon/sprite/social.svg icon/svg/social/*.svg",
    "icon:sprite:touch": "svgstore -o icon/sprite/touch.svg icon/svg/touch/*.svg",
    "icon:sprite:weather": "svgstore -o icon/sprite/weather.svg icon/svg/weather/*.svg",

    "icon:react": "npm run icon:react:brands && npm run icon:react:commerce && npm run icon:react:files && npm run icon:react:general && npm run icon:react:kitchen && npm run icon:react:media && npm run icon:react:real-estate && npm run icon:react:social && npm run icon:react:touch && npm run icon:react:weather",
    "icon:react:brands": "node icons-react --src=icon/svg/brands --dest=icon/js/brands",
    "icon:react:commerce": "node icons-react --src=icon/svg/commerce --dest=icon/js/commerce",
    "icon:react:files": "node icons-react --src=icon/svg/files --dest=icon/js/files",
    "icon:react:general": "node icons-react --src=icon/svg/general --dest=icon/js/general",
    "icon:react:kitchen": "node icons-react --src=icon/svg/kitchen --dest=icon/js/kitchen",
    "icon:react:media": "node icons-react --src=icon/svg/media --dest=icon/js/media",
    "icon:react:real-estate": "node icons-react --src=icon/svg/real-estate --dest=icon/js/real-estate",
    "icon:react:social": "node icons-react --src=icon/svg/social --dest=icon/js/social",
    "icon:react:touch": "node icons-react --src=icon/svg/touch --dest=icon/js/touch",
    "icon:react:weather": "node icons-react --src=icon/svg/weather --dest=icon/js/weather"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.8",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@eslint/js": "*",
    "babel-loader": "^9.2.1",
    "chai": "^5.2.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.15.0",
    "html-webpack-plugin": "^5.6.3",
    "less-loader": "^12.2.0",
    "mocha": "^11.1.0",
    "style-loader": "^4.0.0",
    "svg-url-loader": "^8.0.0",
    "svgo": "^3.3.2",
    "svgstore-cli": "^2.0.1",
    "ts-loader": "^9.5.2",
    "typescript-eslint": "^8.24.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "react": "^19.0.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5",
    "typescript": "^5.7.3"
  }
}
